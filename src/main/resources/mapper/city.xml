<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.traveler.app.repository.CityRepository">

  <select id="findByAll" resultType="com.traveler.app.model.City">
    SELECT * FROM city
  </select>
  
  <select id="findById" resultType="com.traveler.app.model.City">
    SELECT * FROM city WHERE id = #{id}
  </select>
  
</mapper>